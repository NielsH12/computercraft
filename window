os.loadAPI("n")

function window()
	n.forward(1,'f')
	for i = 1,6,1 do
		turtle.dig()
		n.down(1,'f')
	end
	turtle.dig()
	
	n.left()
	n.forward(1,'f')
	n.right()
	
	for i = 1,6,1 do
		turtle.dig()
		n.up(1,'f')
	end
	turtle.dig()
	
	n.left()
	n.forward(1,'f')
	n.right()
	
	for i = 1,6,1 do
		turtle.dig()
		n.down(1,'f')
	end
	
	n.back()
	n.up(6)
	n.right()
	n.forward(2)
	n.left()
end

window()